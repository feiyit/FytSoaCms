                <p class="subtitle">SERVICE</p>
            </div>
            <div class="fyt-category wow movedx animated" data-movedx-mid="2" data-movedx-distance="15">
                <a href="/cases/" class="active" target="_blank"><span>全部</span></a>
                @{
                    var cclass = Model.Column.Where(m => m.ParentId == 1015).ToList();
                    foreach (var item in cclass)
                    {
                        <a href="/cases/@item.EnTitle/" title="@item.Title" target="_blank"><span>@item.Title</span></a>
                    }
                }
            </div>
            <div class="case-list">
                <ul class="clearfix">
                    @{
                        var index = 0;
                        foreach (var item in Model.Case)
                        {
                            <li class="case-row wow fadeInUp" data-wow-delay=".@(index)s">
                                <a href="/cases/detail/@item.Id/" title="@item.Title" target="_blank">
                                    <div class="case-row-img">
                                        <img src="@item.ImgUrl" alt="@item.Title" />
                                    </div>
                                    <div class="case-row-info">
                                        <p class="title">@item.Title</p>
                                        <p class="subtitle">
                                            <span>@item.Tag</span>
                                        </p>
                                        <p class="description">@item.Summary</p>
                                    </div>
                                </a>
                            </li>
                            index++;
                        }
                    }
                </ul>
                <div class="fyt-more wow fadeInUp" data-wow-delay=".7s"><a href="/cases/" title="更多" target="_blank">MORE<i class="glyphicon glyphicon-menu-right"></i></a></div>
            </div>
        </div>

